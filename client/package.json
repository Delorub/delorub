{
  "name": "delorub-frontend",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": "6.9.0",
    "npm": "4.1.1"
  },
  "scripts": {
    "test": "NODE_PATH=./app mocha --compilers js:babel-core/register --require ./app/libs/testHelper.js --require ./app/libs/testNullCompiler.js 'app/**/*.spec.@(js|jsx)'",
    "test:debug": "yarn run test -- --debug-brk",
    "start": "webpack-dev-server",
    "build:production:client": "NODE_ENV=production webpack --config webpack.config.js",
    "build:production:server": "NODE_ENV=production webpack --config webpack.server.config.js",
    "build:client": "webpack --config webpack.config.js",
    "build:dev:client": "NODE_ENV=development webpack -w --config webpack.config.js",
    "build:dev:server": "NODE_ENV=development webpack -w --config webpack.server.config.js",
    "build:server": "webpack --config webpack.server.config.js",
    "build:test": "yarn run build:client && yarn run build:server",
    "build:production": "rm -rf ../public/webpack/production && NODE_ENV=production yarn run build:production:client && yarn run build:production:server",
    "lint": "eslint --ext .js,.jsx .",
    "hardreset": "rm -rf ./node_modules && yarn install && yarn run-script build:client"
  },
  "dependencies": {
    "bootstrap": "v4.0.0-alpha.6",
    "bootstrap-slider": "^9.7.3",
