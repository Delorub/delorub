    "superagent": "^3.5.2",
    "time-input": "^1.4.0",
    "autoprefixer": "^6.5.3",
    "axios": "^0.15.2",
    "babel": "^6.5.2",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.8",
    "babel-plugin-transform-es2015-modules-amd": "^6.18.0",
    "babel-polyfill": "^6.16.0",
